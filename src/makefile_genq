all: desc gene_sql.out

desc:
	@echo "===---===--===\nto general sql\n===---===---===\n"

gene_sql.out: $^
	g++ --std=c++11 -g -c  inifile.cpp -o inifile.o
	g++ --std=c++11 -g -c DbfRead.cpp -o DbfRead.o
	g++ --std=c++11 -g  inifile.o DbfRead.o GeneSql.cpp -o gene_sql.out

clean:
	rm -rf *.o *.out